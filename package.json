{
  "name": "rabaz-team-typescript-template",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "start": "node --async-stack-traces ./dist/index.js",
    "dev": "npm run build && npm start",
    "test": "jest --coverage",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev:server": "NODE_ENV=dev nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc.js 'src/**/*.ts' --check",
    "prettier:fix": "prettier --config .prettierrc.js 'src/**/*.ts' --write",
    "clean": "rimraf dist"
  },
  "author": "Alexander Gurevich",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "env-var": "^7.0.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "helmet": "^4.1.1",
    "joi": "^17.2.1",
    "menashmq": "^0.1.3",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.11.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.5.3",
    "jest": "^27.0.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.3"
  }
}
